@model IEnumerable<WebApplication1.Models.S_Province>
@using WebApplication1.Resources
@{
    ViewBag.Title = Resource.Index;
}

<h2>@Resource.Index</h2>

<p>
    @Html.ActionLink(Resource.Create, "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ProvinceName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateCreated)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateUpdated)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ProvinceName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateCreated)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateUpdated)
        </td>
        <td>
            @Html.ActionLink(Resource.Edit, "Edit", new { id=item.ProvinceID }) |
            @Html.ActionLink(Resource.Details, "Details", new { id=item.ProvinceID }) |
            @Html.ActionLink(Resource.Delete, "Delete", new { id=item.ProvinceID })
        </td>
    </tr>
}

</table>
